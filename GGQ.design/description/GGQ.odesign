  <ownedViewpoints name="GQL Viewpoint">
    <ownedRepresentations xsi:type="description_1:DiagramDescription" name="Gql Diagram" domainClass="gql::GraphQuery" enablePopupBars="true">
      <defaultLayer name="Default">
