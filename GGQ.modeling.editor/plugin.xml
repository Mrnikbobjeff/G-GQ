<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated modeling -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="ggq.gql.presentation.ModelingModelWizardID"
            name="%_UI_ModelingModelWizard_label"
            class="ggq.gql.presentation.ModelingModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/ModelingModelFile.gif">
         <description>%_UI_ModelingModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated modeling -->
      <editor
            id="ggq.gql.presentation.ModelingEditorID"
            name="%_UI_ModelingEditor_label"
            icon="icons/full/obj16/ModelingModelFile.gif"
            extensions="modeling"
            class="ggq.gql.presentation.ModelingEditor"
            contributorClass="ggq.gql.presentation.ModelingActionBarContributor">
      </editor>
   </extension>

</plugin>
